<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AluguelCarro</title>
    <link rel="stylesheet" href="https://bootswatch.com/5/spacelab/bootstrap.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/css/bootstrap-datepicker3.min.css" />
    <link rel="stylesheet" href="https://use.fontawesome.com/realeases/v5.7.2/css/all.css" />

    <link rel="stylesheet" href="~/css/site.css" />
    <script src="https://kit.fontawesome.com/e750d3ce7d.js" ></script>
</head>
<body>
    <header>
        <div class="container-fluid">
            <nav class="navbar navbar-expand-lg navbar-light bg-info">
                <a class="navbar-brand text-white" asp-controller="Usuarios" asp-action="Index"><i class="fas fa-car fa-lg"></i> Aluguel de Carros</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav mr-auto">
                        <li class="nav-item active">
                            <a class="nav-link navbar-text text-white " asp-action="Registro" asp-controller="Usuarios">Registro </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link navbar-text text-white" asp-action="Login" asp-controller="Usuarios">Login</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link navbar-text text-white" asp-action="Index" asp-controller="Usuarios">Meus dados</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link navbar-text text-white" asp-action="Index" asp-controller="Carros">Carros</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link navbar-text text-white" asp-action="index" asp-controller="NiveisAcessos">Niveis de acesso</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link navbar-text text-white" asp-action="Index" asp-controller="Contas">Saldo</a>
                        </li>


                    </ul>
                    <partial name="_LoginPartial" />


                    
                </div>
            </nav>


        </div>
    </header>
    <div class="container">
        <div class="conteudoPrincipal">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - AluguelCarro - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.1.0/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/2.9.3/umd/popper.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/locales/bootstrap-datepicker.pt-BR.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/js/bootstrap-datepicker.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.1/moment.min.js"></script>

    


    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
